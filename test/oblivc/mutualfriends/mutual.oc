void addString(obliv char* src, obliv char* dest) obliv{
     int i;
     for(i=0; i<MAXL; i++)
        dest[i]=src[i];
}

void swap(obliv char list[MAXN][MAXL], int i, int j) obliv{
    obliv char temp[MAXL];
    addString(list[i], temp);
    addString(list[j], list[i]);
    addString(temp, list[j]);
}

